<launch>
  
  <group ns="head1">
    <master auto="start"/>
    <!-- Load the Base Parameter -->
    <rosparam command="load" file="$(find roborts_base)/config/base1.yaml" />
    <!-- Run the Base Node -->
    <node pkg="roborts_base" type="roborts_base_node" name="roborts_base_node" output="screen" respawn="true" />
  </group>

  <group ns="head2">
    <master auto="start"/>
    <!-- Load the Base Parameter -->
    <rosparam command="load" file="$(find roborts_base)/config/base2.yaml" />
    <!-- Run the Base Node -->
    <node pkg="roborts_base" type="roborts_base_node" name="roborts_base_node" output="screen" respawn="true" />
  </group>

  

</launch>

